{
  "name": "second-view",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:content": "rm -rf dist/content",
    "clean:popup": "rm -rf dist/popup",
    "dev:content": "vite --config vite.config.content.js",
    "dev:popup": "vite --config vite.config.popup.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc -b",
    "build": "npm run lint && npm run type-check && npm run build:content && npm run build:popup",
    "build:content": "npx vite build --config vite.config.content.js",
    "build:popup": "npx vite build --config vite.config.popup.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/chrome": "^0.1.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.4",
    "vite-plugin-static-copy": "^3.1.1"
  }
}
